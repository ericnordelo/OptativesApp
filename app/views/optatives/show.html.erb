 <div class="row">
    <div class="col-md-12">
      
      <h2>Optativo: <span style="color: crimson"><%= @optative.title %></span></h2>
    </div>
  </div>
  <hr>

<div class="row">
                            <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                                <a class="dashboard-stat dashboard-stat-v2 blue" href="#">
                                    <div class="visual">
                                        <i class="fa fa-comments"></i>
                                    </div>
                                    <div class="details">
                                        <div class="number">
                                            <span data-counter="counterup" data-value="<%= @optative.enrollment%>">0</span>
                                        </div>
                                        <div class="desc"> Matrícula Total </div>
                                    </div>
                                </a>
                            </div>
                            <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                                <a class="dashboard-stat dashboard-stat-v2 red" href="#">
                                    <div class="visual">
                                        <i class="fa fa-bar-chart-o"></i>
                                    </div>
                                    <div class="details">
                                        <div class="number">
                                            <span data-counter="counterup" data-value="12">0</span></div>
                                        <div class="desc"> Matrícula Ocupada </div>
                                    </div>
                                </a>
                            </div>
                            <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                                <a class="dashboard-stat dashboard-stat-v2 green" href="#">
                                    <div class="visual">
                                        <i class="fa fa-shopping-cart"></i>
                                    </div>
                                    <div class="details">
                                        <div class="number">
                                            <span data-counter="counterup" data-value="18">0</span>
                                        </div>
                                        <div class="desc"> Matrícula Disponible </div>
                                    </div>
                                </a>
                            </div>
                            <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                                <a class="dashboard-stat dashboard-stat-v2 purple" href="#">
                                    <div class="visual">
                                        <i class="fa fa-globe"></i>
                                    </div>
                                    <div class="details">
                                        <div class="number">
                                            <span data-counter="counterup" data-value="<%= @optative.duration%>"></span> horas </div>
                                        <div class="desc"> Duración </div>
                                    </div>
                                </a>
                            </div>
                        </div>

<div class="container-fluid">
 
  <div class="row">
    <div class="col-md-8">
      
      <h3><%= @optative.description %></h3>
    </div>
  </div>
</div>
<br><br>
  
<table class="table table-hover">
  <tr>
    <td><strong>Profesor:</strong></td>
    <td><%= @optative.professor %></td>
  </tr>
  <tr>
    <td><strong>Duración:</strong></td>
    <td><%= @optative.schedule %></td>
  </tr>
  <tr>
    <td><strong>Año:</strong></td>
    <td> <%= @optative.year %></td>
  </tr>
  <tr>
    <td><strong>Carrera:</strong></td>
    <td><%= @optative.program %></td>
  </tr>
  <tr>
    <td><strong>Matrícula:</strong></td>
    <td><%= @optative.enrollment %></td>
  </tr>
</table>

