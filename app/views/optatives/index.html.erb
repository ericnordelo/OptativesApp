<p id="notice"><%= notice %></p>

<h1>Optatives</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Professor</th>
      <th>Duration</th>
      <th>Schedule</th>
      <th>Year</th>
      <th>Program</th>
      <th>Enrollment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @optatives.each do |optative| %>
      <tr>
        <td><%= optative.title %></td>
        <td><%= optative.description %></td>
        <td><%= optative.professor %></td>
        <td><%= optative.duration %></td>
        <td><%= optative.schedule %></td>
        <td><%= optative.year %></td>
        <td><%= optative.program %></td>
        <td><%= optative.enrollment %></td>
        <td><%= link_to 'Show', optative %></td>
        <td><%= link_to 'Edit', edit_optative_path(optative) %></td>
        <td><%= link_to 'Destroy', optative, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Optative', new_optative_path %>
